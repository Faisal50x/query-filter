{
    "name": "faisal50x/query-filter",
    "description": "Laravel Eloquent models filter based on query parameters.",
    "keywords": [
        "faisal50x",
        "query-filter",
        "laravel",
        "query",
        "filters",
        "eloquent"
    ],
    "homepage": "https://github.com/faisal50x/query-filter",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Faisal Ahmed",
            "email": "hello@imfaisal.me",
            "homepage": "https://github.com/Faisal50x",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1|^7.2|^7.3|^7.4|^8.0|^8.1",
        "illuminate/console": "^5.6|^6.0|^7.0|^8.0|^9.0|^10.0",
        "illuminate/support": "^5.6|^6.0|^7.0|^8.0|^9.0|^10.0",
        "illuminate/database": "^5.6|^6.0|^7.0|^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0|^5.0|^6.0|^7.0|^8.0",
        "phpunit/phpunit": "^8.0|^9.0|^10.0",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "Faisal50x\\QueryFilter\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Faisal50x\\QueryFilter\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Faisal50x\\QueryFilter\\QueryFilterServiceProvider"
            ]
        }
    }
}
